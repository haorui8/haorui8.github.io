{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  
    "title":{
      "text":"Obesity in England",
      "subtitle":"Admissions of obesity, categorised by gender",
      "subtitleFontStyle":"italic",
      "subtitleFontSize":10,
      "anchor": "start",
      "color":"black"
    },
  
    "data":{
      "url": "https://raw.githubusercontent.com/haorui8/haorui8.github.io/main/Project/ObesityMaleFemale.csv"
    },
  
    "repeat": {"layer":["Male", "Female"]},
    "spec":{
      "mark":"bar",
      "encoding": {
        "x":{
          "field":"Year",
          "type": "nominal",
          "title":null,
          "axis":{"labelAngle":45}
        },
        "y":{
          "field":{"repeat": "layer"},
          "type": "quantitative",
          "aggregate":"sum",
          "title":"Number of persons"
        },
        "color":{
          "datum":{"repeat":"layer"}
        },
        "xOffset":{
          "datum":{"repeat":"layer"}
        },
        "tooltip":[{
          "field":"Male", "type": "quantitative", "title":"number of male"
  },
  {"field":"Female", "type": "quantitative","title":"number of female"}
  ]
      }
    }
  
  
  }